{
  "name": "react-spa-boilerplate",
  "version": "0.1.0",
  "description": "",
  "author": "Yeomin Kwon",
  "scripts": {
    "build": "npm run clean-dist && webpack -p --config=configs/webpack/prod.js",
    "build:dev": "npm run clean-dist && webpack -p --config=configs/webpack/prod.js --watch",
    "clean-dist": "rm -f -r -d client",
    "lint": "npm run lint:ts && npm run lint:sass",
    "lint:ts": "tslint './src/**/*.ts*' --format stylish --force",
    "lint:sass": "stylelint ./src/**/*.scss --fix",
    "start": "npm run start:dev",
    "start:dev": "webpack-dev-server --config=configs/webpack/dev.js",
    "start:prod": "npm run build && node express.js",
    "test": "jest -i --no-cache --watch --config=configs/jest.json",
    "e2e:local": "./node_modules/.bin/wdio configs/webdriverio/local.conf.js",
    "e2e:single": "./node_modules/.bin/wdio configs/webdriverio/single.conf.js",
    "e2e:parallel": "./node_modules/.bin/wdio configs/webdriverio/parallel.conf.js",
    "e2e:multiple": "./node_modules/.bin/wdio configs/webdriverio/multiple.conf.js"
  },
